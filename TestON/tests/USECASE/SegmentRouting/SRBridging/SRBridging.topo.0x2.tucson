        <NetworkBench>
            <host>10.76.28.66</host>
            <user>jenkins</user>
            <password></password>
            <type>NetworkDriver</type>
            <connect_order>10</connect_order>
            <COMPONENTS>
            </COMPONENTS>
        </NetworkBench>

    </COMPONENT>
