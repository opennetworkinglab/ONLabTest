                        <interface>pairbond</interface>
                    </route1>
                </routes>
                <sudo_required>true</sudo_required>
            </COMPONENTS>
        </Compute2>

        <Compute3>
            <host>10.76.28.68</host>
            <user>jenkins</user>
            <password></password>
            <type>HostDriver</type>
            <connect_order>8</connect_order>
            <jump_host></jump_host>
            <COMPONENTS>
                <mac></mac>
                <inband>false</inband>
                <dhcp>True</dhcp>
